{
  "env": {
    "browser" : true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "extends": [
    "eslint:recommended",
    "plugin:prettier/recommended"
    ],
    "plugins": [
    "html",
    "prettier"
    ],
    "rules": {
    "no-console": "off"
    }
}